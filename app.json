{
    "name": "CraftCMS",
    "description": "CraftCMS",
    "repository": "https://github.com/dnunez24/craftcms-project-heroku",
    "success_url": "/",
    "keywords": [
        "craftcms",
        "craft cms",
        "craft",
        "cms"
    ],
    "scripts": {
        "postdeploy": "bin/schematic import"
    },
    "env": {
        "CRAFT_USERNAME": {
            "description": "Admin username",
            "required": true
        },
        "CRAFT_EMAIL": {
            "description": "Admin user email",
            "required": true
        },
        "CRAFT_PASSWORD": {
            "description": "Admin user password",
            "required": true,
            "generator": "secret"
        },
        "CRAFT_SITENAME": {
            "description": "Human friendly name for the site",
            "required": true
        },
        "CRAFT_SITEURL": {
            "description": "Base URL for the website (must match Heroku app URL)",
            "required": true
        },
        "CRAFT_LOCALE": {
            "description": "Default locale for the admin panel",
            "required": true,
            "value": "en_US"
        }
    },
    "formation": {
        "web": {
            "quantity": 1,
            "size": "free"
        }
    },
    "stack": "cedar-16",
    "image": "heroku/php",
    "addons": [{
        "plan": "heroku-redis:hobby-dev"
    }, {
        "plan": "jawsdb-maria:kitefin"
    }, {
        "plan": "sendgrid:starter"
    }, {
        "plan": "newrelic:wayne"
    }, {
        "plan": "papertrail:choklad"
    }],
    "buildpacks": [{
        "url": "heroku/php"
    }, {
        "url": "heroku/nodejs"
    }],
    "environments": {}
}
